{
   "manifest_version":2,
   "name": "Audio & Video Grabber",
   "background": {
      "scripts":["mediaRequestProbe.js"]
   },
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "browser_action": {
      "default_icon": "UI/img/128x_icon_off.png",
      "default_title":"Download/Transcode Audio&Videos",
      "default_popup": "popup.html"
   },
   "description": "Download and transcode audio&videos",
   "icons": {
      "128": "UI/img/128x_icon_off.png",
      "16": "UI/img/18x_icon_off.png",
      "48": "UI/img/128x_icon_off.png"
   },
   "permissions": ["webRequest", "webRequestBlocking", "tabs","notifications",
                   "<all_urls>"],
   "options_page": "options.html",
   "version": "0.0.1"
}
